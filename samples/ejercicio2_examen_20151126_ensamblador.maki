; EJERCICIO 2 EXAMEN 20151126 ENSAMBLADOR
; =======================================
BEGIN-DATA
		A=7
		B=3
		R=?
END-DATA
	
BEGIN-CODE
	
		CMP [A],5
		JGT FUN1
		JLE FUN2

FUN1:	CMP [B],3
		JLT SIGUE1
		JGE SIGUE2
		JE SIGUE2

SIGUE1:	MUL [A],[B]
		SUB 5
		MOV AC,[R]
		TERM

FUN2:	ADD [A],2
		MOV AC,[R]
		TERM

SIGUE2:	MOV 1,[R]
		TERM

END-CODE