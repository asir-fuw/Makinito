; EJERCICIO 2 EXAMEN 20151126 ENSAMBLADOR
; =======================================
BEGIN-DATA
		A=7
		B=3
		R=?
END-DATA
	
BEGIN-CODE
	
		COMPARAR [A],5
		SALTAR-SI-MA FUN1
		SALTAR-SI-MEI FUN2

FUN1:	COMPARAR [B],3
		SALTAR-SI-ME SIGUE1
		SALTAR-SI-MAI SIGUE2
		SALTAR-SI-IG SIGUE2

SIGUE1:	MULTIPLICAR [A],[B]
		RESTAR 5
		MOVER AC,[R]
		TERMINAR

FUN2:	SUMAR [A],2
		MOVER AC,[R]
		TERMINAR

SIGUE2:	MOVER 1,[R]
		TERMINAR

END-CODE